<?xml version="1.0" encoding="UTF-8" ?>

<!--
	basic type mapping for the framework 'phpdoctrine'
	
	@author	tobias haubold
	@author	peter huster (last)
	@date	2008-11-27
-->

<tns:typeMapping xmlns:tns="http://www.genesez.org/typemapping"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:include>
    <tns:file>org/genesez/platform/php/typemapping/typemapping.xml</tns:file>
  </tns:include>
  <tns:primitiveTypes>
  	<!--
		primitive types, defined within the UML metamodel
		these are already in the php.typemapping.xml defined
	-->
	<tns:type from="Boolean">
		<tns:to>boolean</tns:to>
	</tns:type>
	<tns:type from="String">
		<tns:to>string</tns:to>
	</tns:type>
	<tns:type from="Integer">
		<tns:to>integer</tns:to>
	</tns:type>
	<tns:type from="UnlimitedNatural">
		<tns:to>integer</tns:to>
	</tns:type>
  </tns:primitiveTypes>
  <!--
		further types need to be modeled as external types
	-->
  <tns:externalTypes>
  	<tns:type from="Float">
		<tns:to>float</tns:to>
	</tns:type>
	<tns:type from="Array">
		<tns:to>array</tns:to>
	</tns:type>
	<tns:type from="Object">
		<tns:to>object</tns:to>
	</tns:type>
	<tns:type from="Blob">
		<tns:to>blob</tns:to>
	</tns:type>
	<tns:type from="Clob">
		<tns:to>clob</tns:to>
	</tns:type>
	<tns:type from="Timestamp">
		<tns:to>timestamp</tns:to>
	</tns:type>
	<tns:type from="time">
		<tns:to>time</tns:to>
	</tns:type>
	<tns:type from="Date">
		<tns:to>date</tns:to>
	</tns:type>
	<tns:type from="Enum">
		<tns:to>enum</tns:to>
	</tns:type>
	<tns:type from="Gzip">
		<tns:to>gzip</tns:to>
	</tns:type>
  </tns:externalTypes>
</tns:typeMapping>
