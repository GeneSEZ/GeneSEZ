<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="platforms" name="platforms" schema="schema/platform.exsd"/>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="de.genesez.core.newWizards"
            name="GeneSEZ">
      </category>
      <wizard
            category="de.genesez.core.newWizards"
            class="de.genesez.core.wizard.ProjectWizard"
            finalPerspective="org.openarchitectureware.base.oAWPerspective"
            icon="icons/genesez.gif"
            id="de.genesez.core.GeneratorProjectWizard"
            name="GeneSEZ Project"
            preferredPerspectives="org.openarchitectureware.base.oAWPerspective"
            project="true">
      </wizard>
      <wizard
            category="de.genesez.core.newWizards"
            class="de.genesez.core.wizard.PlatformWizard"
            finalPerspective="org.openarchitectureware.base.oAWPerspective"
            icon="icons/genesez.gif"
            id="de.genesez.core.PlatformProjectWizard"
            name="GeneSEZ Platform"
            preferredPerspectives="org.openarchitectureware.base.oAWPerspective"
            project="true">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.emf.ecore.extension_parser">
      <parser
            class="de.genesez.core.project.resource.projectResourceFactory"
            type="dsl">
      </parser>
   </extension>

</plugin>
