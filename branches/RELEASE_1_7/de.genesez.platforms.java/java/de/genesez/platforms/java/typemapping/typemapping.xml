		</tns:type>
		<tns:type from="Date">
			<tns:to>Date</tns:to>
			<tns:context name="Import">java.util.Date</tns:context>
		</tns:type>
		<tns:type from="HashSet">
			<tns:to>HashSet</tns:to>
			<tns:context name="Import">java.util.HashSet</tns:context>
		</tns:type>
