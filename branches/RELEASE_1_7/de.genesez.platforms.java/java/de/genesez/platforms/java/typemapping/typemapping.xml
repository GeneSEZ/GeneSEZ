			<tns:context name="Wrapper">Byte</tns:context>
		</tns:type>
		<tns:type from="double">
			<tns:to>double</tns:to>
			<tns:context name="Wrapper">double</tns:context>
		</tns:type>
		<tns:type from="float">
			<tns:to>float</tns:to>
			<tns:context name="Wrapper">Float</tns:context>
		</tns:type>
		<tns:type from="int">
			<tns:to>int</tns:to>
