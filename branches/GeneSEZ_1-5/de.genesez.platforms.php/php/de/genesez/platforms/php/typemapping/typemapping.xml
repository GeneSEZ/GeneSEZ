<?xml version="1.0" encoding="UTF-8" ?>
<!--
	basic type mapping for php
	
	@author	tobias haubold
	@date	2008-03-27
 -->
<typeMapping>
	<header>
		<superMappingFile>../de.genesez.platforms.common/common/de/genesez/platforms/common/typemapping/typeMapping.xml</superMappingFile>
	</header>
	
	<collectionTypes>
	</collectionTypes>
	
	<primitiveTypes>
		<!-- uml metamodel primitives -->
		<mappingElement>
			<map>Boolean</map>
			<defaultType>boolean</defaultType>
		</mappingElement>
		<mappingElement>
			<map>String</map>
			<defaultType>string</defaultType>
		</mappingElement>
		<mappingElement>
			<map>Integer</map>
			<defaultType>int</defaultType>
		</mappingElement>
		<mappingElement>
			<map>UnlimitedNatural</map>
			<defaultType>int</defaultType>
		</mappingElement>
		
		<!-- magic draw specific primitives -->
		<mappingElement>
			<map>void</map>
			<defaultType>null</defaultType>
		</mappingElement>
	</primitiveTypes>
	
	<externalTypes>
		<!-- exception is a primitive type from php -->
		<mappingElement>
			<map>Exception</map>
			<defaultType>Exception</defaultType>
		</mappingElement>
	</externalTypes>
	
</typeMapping>
