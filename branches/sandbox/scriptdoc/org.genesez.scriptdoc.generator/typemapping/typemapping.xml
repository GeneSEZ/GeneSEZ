<?xml version="1.0" encoding="UTF-8" ?>
<!--
	example for a project specific typemapping
	
	@author	tobias haubold
	@author	peter huster (last)
	@date	2008-11-27
 -->
<tns:typeMapping xmlns:tns="http://www.genesez.de/typemapping"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<tns:include>
		<tns:file>org/genesez/platforms/java/typemapping/typemapping.xml</tns:file>
	</tns:include>
	<tns:externalTypes>
		<tns:type from="Path">
			<tns:to>File</tns:to>
			<tns:context name="Import">java.io.File</tns:context>
		</tns:type>
		<tns:type from="File">
			<tns:to>File</tns:to>
			<tns:context name="Import">java.io.File</tns:context>
		</tns:type>
		<tns:type from="SymbolTable">
			<tns:to>TreeMap &lt;String, LinkedHashSet&lt;Executable&gt; &gt;</tns:to>
			<tns:context name="Import">java.util.TreeMap</tns:context>
			<tns:context name="Import">java.util.LinkedHashSet</tns:context>
		</tns:type>
		<tns:type from="SymbolList">
			<tns:to>LinkedHashSet&lt;Executable&gt;</tns:to>
			<tns:context name="Import">java.util.LinkedHashSet</tns:context>
		</tns:type>
		<tns:type from="Pattern">
			<tns:to>Pattern</tns:to>
			<tns:context name="Import">java.util.regex.Pattern</tns:context>
		</tns:type>
		<tns:type from="Matcher">
			<tns:to>Matcher</tns:to>
			<tns:context name="Import">java.util.regex.Matcher</tns:context>
		</tns:type>
	</tns:externalTypes>
</tns:typeMapping>
