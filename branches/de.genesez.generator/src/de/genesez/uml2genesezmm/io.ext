// simple helper file that can be used to
// "debug" transformations by printing all
// kinds of objects to System.err
// it delegates to a Java class called 
// IOExtensions

private Void internalDebug(Object s) :
   JAVA de.genesez.uml2genesezmm.utilities.IOExtensions.debug(java.lang.Object)
;

private Void internalInfo(Object s) :
   JAVA de.genesez.uml2genesezmm.utilities.IOExtensions.info(java.lang.Object)
;
 
private Void internalError(Object s) :
   JAVA de.genesez.uml2genesezmm.utilities.IOExtensions.error(java.lang.Object)
;

debug(Object o) :
   internalDebug(o) -> o
;

info(Object o) :
   internalInfo(o) -> o
;

error(Object o) :
   internalError(o) -> o
;