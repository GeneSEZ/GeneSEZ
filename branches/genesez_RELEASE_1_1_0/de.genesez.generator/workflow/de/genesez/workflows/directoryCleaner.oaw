<?xml version="1.0" encoding="utf-8"?>

<workflow>
	
	<!-- 
		This workflow component is provided to clean some directories for 
		testing code.
		
		Following parameters are required:
		==================================
		
		$cleaningDirs		directories to clean, comma separated list with directory names
	 -->
	
	<component id="dirCleaner"
		class="oaw.workflow.common.DirectoryCleaner"
		directories="${cleaningDirs}">
	</component>

</workflow>
