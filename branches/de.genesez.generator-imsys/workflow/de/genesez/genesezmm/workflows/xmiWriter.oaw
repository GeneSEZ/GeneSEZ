<?xml version="1.0" encoding="utf-8"?>

<workflow>
	
	<!-- 	This workflow component is provided to write a ecore file.
			
			Following parameters are required:
			----------------------------------
			
			$outputModelFile: 	name of the ecore file
	 -->	

	<!-- Write the ecore model into the ecore file. -->
	<component id="xmiWriter" class="org.openarchitectureware.emf.XmiWriter">
		<inputSlot value="generatorModel" />
		<modelFile value="${outputModelFile}" />
	</component>

</workflow>
