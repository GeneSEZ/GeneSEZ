		value="${basedir}"
		else="${workspace.location}/de.genesez.build">
		<matches pattern="de.genesez.build" string="${ant.file}" />
	</condition>

	<import file="environment.xml"/>

	<import file="classpath.xml"/>
	<import file="antcontrib.xml" />
	<import file="subversion.xml" />

</project>
