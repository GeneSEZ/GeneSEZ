<?xml version="1.0" encoding="UTF-8" ?>
<!--
	basic type mapping for Java ME CLDC 1.x
	
	@author	nico herbig
	@author	tobias haubold
	@author	peter huster (last)
	@date	2008-11-27
 -->
 <tns:typeMapping xmlns:tns="http://www.genesez.org/typemapping"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:include>
  	<!-- type mapping is a specialized version of the standard java typemapping -->
    <tns:file>org/genesez/platforms/java/typemapping/typemapping.xml</tns:file>
  </tns:include>
  <tns:multiValuedTypes>
  <!-- map all multi-valued types to vector -->
    <tns:multiValuedType>
      <tns:default>java.util.Vector</tns:default>
      <tns:context name="Implementation">java.util.Vector</tns:context>
    </tns:multiValuedType>
    <tns:multiValuedType ordered="false" unique="false">
      <tns:default>java.util.Vector</tns:default>
	   <tns:context name="Implementation">java.util.Vector</tns:context>
    </tns:multiValuedType>
    <tns:multiValuedType unique="false" ordered="true">
       <tns:default>java.util.Vector</tns:default>
	   <tns:context name="Implementation">java.util.Vector</tns:context>
    </tns:multiValuedType>
	<tns:multiValuedType ordered="true" unique="false">
      <tns:default>java.util.Vector</tns:default>
      <tns:context name="Implementation">java.util.Vector</tns:context>
    </tns:multiValuedType>
	<tns:multiValuedType ordered="true" unique="true">
      <tns:default>java.util.Vector</tns:default>
      <tns:context name="Implementation">java.util.Vector</tns:context>
    </tns:multiValuedType>
  </tns:multiValuedTypes>
</tns:typeMapping>
