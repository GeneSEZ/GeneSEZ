<?xml version="1.0" encoding="UTF-8" ?>
<!--
	basic type mapping for Java ME CLDC 1.x
	
	@author	nico herbig
	@author	tobias haubold (last)
	@date	2008-02-19
 -->
<typeMapping>
	
	<!-- type mapping is a specialized version of the standard java typemapping -->
	<header>
		<superMappingFile>../de.genesez.platforms.java/java/de/genesez/platforms/java/typemapping/typeMapping4Java.xml</superMappingFile>
	</header>
	
	<!-- map all multi-valued types to vector -->
	<collectionTypes>
		<mappingElement>
			<defaultType>java.util.Vector</defaultType>
			<contextType>
				<context>Implementation</context>
				<type>java.util.Vector</type>
			</contextType>
		</mappingElement>
		<mappingElement unique="false" ordered="false">
			<defaultType>java.util.Vector</defaultType>
			<contextType>
				<context>Implementation</context>
				<type>java.util.Vector</type>
			</contextType>
		</mappingElement>
		<mappingElement unique="false" ordered="true">
			<defaultType>java.util.Vector</defaultType>
			<contextType>
				<context>Implementation</context>
				<type>java.util.Vector</type>
			</contextType>
		</mappingElement>
		<mappingElement unique="true" ordered="false">
			<defaultType>java.util.Vector</defaultType>
			<contextType>
				<context>Implementation</context>
				<type>java.util.Vector</type>
			</contextType>
		</mappingElement>
		<mappingElement unique="true" ordered="true">
			<defaultType>java.util.Vector</defaultType>
			<contextType>
				<context>Implementation</context>
				<type>java.util.Vector</type>
			</contextType>
		</mappingElement>
	</collectionTypes>
	
	<primitiveTypes>
	</primitiveTypes>
	
	<externalTypes>
	</externalTypes>

</typeMapping>
