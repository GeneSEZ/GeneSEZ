<?xml version="1.0" encoding="UTF-8" ?>
<!--
	type mappings for the pear auth framework
	
	@author	toh
	@date	2009-06-24
 -->
<tns:typeMapping xmlns:tns="http://www.genesez.org/typemapping"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<tns:include>
		<tns:file>org/genesez/platform/php/typemapping/typemapping.xml</tns:file>
	</tns:include>
	<tns:externalTypes>
		<tns:type from="Auth">
			<tns:to>Auth</tns:to>
			<tns:context name="import">Auth.php</tns:context>
		</tns:type>
		<tns:type from="Auth_Container">
			<tns:to>Auth_Container</tns:to>
			<tns:context name="import">Auth/Container.php</tns:context>
		</tns:type>
	</tns:externalTypes>
</tns:typeMapping>
