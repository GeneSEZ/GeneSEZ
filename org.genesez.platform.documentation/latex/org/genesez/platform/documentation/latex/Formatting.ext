import gcore;

String format(String this) :
	replaceAll("<b><i>([\\w\\s\\d]+)</i></b>", "\\\\emph{$1}")
	.replaceAll("<pre>([\\w\\s\\d]+)</pre>", "\\\\verb|$1|")
	.replaceAll("\\s'([\\w\\s\\d]+)'", " \\\\emph{$1} ")
	.replaceAll("_", "\\\\_");


String metaTypeName(MClass this) :
	"Class";
	
String metaTypeName(MInterface this) :
	"Interface";

String metaTypeName(MEnumeration this) :
	"Enumeration";

String metaTypeName(MClassifier this) :
	"Classifier";