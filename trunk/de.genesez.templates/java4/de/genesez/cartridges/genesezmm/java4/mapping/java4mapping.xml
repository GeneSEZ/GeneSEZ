<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE TemplateMapping SYSTEM "../de.genesez.templates/mapping/de/genesez/mapping/templatemapping.dtd">

<TemplateMapping>
	<Header>
		<SuperMappingFile>../de.genesez.templates/mapping/de/genesez/mapping/mapping.xml</SuperMappingFile>
	</Header>
    
    <Mapping>
        <Map>List</Map>
        <DefaultType>java.util.List</DefaultType>
        <ContextType>
        	<Context>Implementation</Context>
        	<Type>java.util.ArrayList</Type>
        </ContextType>
    </Mapping>
    
    <Mapping>
        <Map>Set</Map>
        <DefaultType>java.util.Set</DefaultType>
        <ContextType>
        	<Context>Implementation</Context>
        	<Type>java.util.HashSet</Type>
        </ContextType>
    </Mapping>
    
    <Mapping>
        <Map>Map</Map>
        <DefaultType>java.util.Map</DefaultType>
        <ContextType>
        	<Context>Implementation</Context>
        	<Type>java.util.HashMap</Type>
        </ContextType>
    </Mapping>
    
    <Mapping>
        <Map>Boolean</Map>
        <DefaultType>boolean</DefaultType>
    </Mapping>
    
</TemplateMapping>
