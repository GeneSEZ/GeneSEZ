	@author	tobias haubold
	@date	2008-04-17
-->
<typeMapping>
	<header>
		<superMappingFile>de/genesez/platforms/php/typemapping/typeMapping.xml</superMappingFile>
	</header>
	
	<collectionTypes>
	</collectionTypes>
	
	<primitiveTypes>
	</primitiveTypes>
	
	<externalTypes>
		<!-- container is the base class for custom authentication providers from 'PEAR::Auth' package -->
		<mappingElement>
			<map>Auth_Container</map>
			<!-- default mapping is to the class name -->
			<defaultType>Auth_Container</defaultType>
			<contextType>
				<!-- used to generate imports -->
				<context>import</context>
				<type>Auth/Container.php</type>
			</contextType>
		</mappingElement>
	</externalTypes>
	
</typeMapping>
