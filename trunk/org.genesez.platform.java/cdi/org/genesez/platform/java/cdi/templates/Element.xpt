«REM» 
	evaluates the stereotype "cdiObserves"
«ENDREM»
«DEFINE Observes FOR MElement-»		
	«IF hasStereotype("cdiObserves")-»
		@Observes(
			«IF	hasUtilizableValue("cdiObserves", "during")-»
				during=«getTaggedValue("cdiObserves", "during")-»
				«IF hasUtilizableValue("cdiObserves", "notifyObserver")-»
				    ,notifyObserver=«getTaggedValue("cdiObserves", "notifyObserver")-»
				«ENDIF-»
		    «ELSE-»
		        «IF hasUtilizableValue("cdiObserves", "notifyObserver")-»
                    notifyObserver=«getTaggedValue("cdiObserves", "notifyObserver")-»
                «ENDIF-»  
			«ENDIF-»		
		)
	«ENDIF-»		
«ENDDEFINE»