<?xml version="1.0" encoding="UTF-8" ?>
<!--
	example for a project specific typemapping
	
	@author	tobias haubold
	@date	2008-07-03
-->
<typeMapping>
	<header>
		<superMappingFile>de/genesez/platforms/java/typemapping/typemapping.xml</superMappingFile>
	</header>
	
	<collectionTypes>
	</collectionTypes>
	
	<primitiveTypes>
	</primitiveTypes>
	
	<externalTypes>
		<!-- we used the type 'Color' and declared it as 'external' within the uml model -->
		<mappingElement>
			<map>Color</map>
			<!-- default mapping should return the type name -->
			<defaultType>Color</defaultType>
			<contextType>
				<!-- used to generate imports -->
				<context>Import</context>
				<type>java.awt.Color</type>
			</contextType>
		</mappingElement>
	</externalTypes>
	
</typeMapping>
