	<!-- Collection for multi value type support -->
	<tns:multiValuedTypes>
		<tns:multiValuedType>
			<tns:default>Tx_Extbase_Persistence_ObjectStorage</tns:default>
			<tns:context name="Repository">Tx_Extbase_Persistence_QueryResultInterface</tns:context>
		</tns:multiValuedType>
		<tns:multiValuedType ordered="false" unique="false">
			<tns:default>Tx_Extbase_Persistence_ObjectStorage</tns:default>
			<tns:context name="Repository">Tx_Extbase_Persistence_QueryResultInterface</tns:context>
		</tns:multiValuedType>
		<tns:multiValuedType ordered="false" unique="true">
			<tns:default>Tx_Extbase_Persistence_ObjectStorage</tns:default>
			<tns:context name="Repository">Tx_Extbase_Persistence_QueryResultInterface</tns:context>
		</tns:multiValuedType>
		<tns:multiValuedType ordered="true" unique="false">
			<tns:default>Tx_Extbase_Persistence_ObjectStorage</tns:default>
			<tns:context name="Repository">Tx_Extbase_Persistence_QueryResultInterface</tns:context>
		</tns:multiValuedType>
