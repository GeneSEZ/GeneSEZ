<?xml version="1.0"?>
<project name="org.genesez.build.antcontrib" basedir="..">

	<!--  all properties are in build.properties -->
	<property file="${project.build.dir}/antcontrib.properties" />

	<!-- path to the ant contrib libraries -->
	<path id="antcontrib.classpath">
		<pathelement location="${antcontrib.jar}" />
	</path>

	<!-- load the ant contrib tasks -->
	<taskdef resource="net/sf/antcontrib/antcontrib.properties"
		classpathref="antcontrib.classpath"/>

</project>
