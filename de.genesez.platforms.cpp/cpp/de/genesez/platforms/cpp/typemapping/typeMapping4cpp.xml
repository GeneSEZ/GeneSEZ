<?xml version="1.0" encoding="UTF-8" ?>
<!--
	basic type mapping for java
	
	@author	nico herbig
	@author	tobias haubold (last)
	@date	2008-02-19
 -->
<typeMapping>
	<header>
		<superMappingFile>de/genesez/platforms/common/typemapping/typeMapping.xml</superMappingFile>
	</header>	
	<collectionTypes>
		<mappingElement>
			<defaultType>std::list</defaultType>
			<contextType>
				<context>Include</context>
				<type>list</type>
			</contextType>			
		</mappingElement>
		<mappingElement unique="false" ordered="false">
			<defaultType>std::list</defaultType>
			<contextType>
				<context>Include</context>
				<type>list</type>
			</contextType>
		</mappingElement>
		<mappingElement unique="false" ordered="true">
			<defaultType>std::list</defaultType>			
			<contextType>
				<context>Include</context>
				<type>list</type>
			</contextType>
		</mappingElement>
		<mappingElement unique="true" ordered="false">
			<defaultType>std::set</defaultType>
			<contextType>
				<context>Include</context>
				<type>set</type>
			</contextType>
		</mappingElement>
		<mappingElement unique="true" ordered="true">
			<defaultType>std::set</defaultType>
			<contextType>
				<context>Include</context>
				<type>set</type>
			</contextType>
		</mappingElement>
	</collectionTypes>		
	<primitiveTypes>
		<mappingElement>
			<map>boolean</map>
			<defaultType>bool</defaultType>			
		</mappingElement>
		<mappingElement>
			<map>Boolean</map>
			<defaultType>bool</defaultType>			
		</mappingElement>
		<mappingElement>
			<map>char</map>
			<defaultType>char</defaultType>
		</mappingElement>
		<mappingElement>
			<map>byte</map>
			<defaultType>char</defaultType>
		</mappingElement>
		<mappingElement>
			<map>double</map>
			<defaultType>double</defaultType>			
		</mappingElement>
		<mappingElement>
			<map>float</map>
			<defaultType>float</defaultType>
		</mappingElement>
		<mappingElement>
			<map>int</map>
			<defaultType>int</defaultType>
		</mappingElement>
		<mappingElement>
			<map>Integer</map>
			<defaultType>int</defaultType>
		</mappingElement>
		<mappingElement>
			<map>long</map>
			<defaultType>long</defaultType>			
		</mappingElement>
		<mappingElement>
			<map>short</map>
			<defaultType>short int</defaultType>
		</mappingElement>
		<mappingElement>
			<map>void</map>
			<defaultType>void</defaultType>
		</mappingElement>
		<mappingElement>
			<map>String</map>
			<defaultType>std::string</defaultType>
			<contextType>
				<context>Include</context>
				<type>string</type>
			</contextType>
		</mappingElement>
	</primitiveTypes>
	
	<externalTypes>
		<mappingElement>
			<map>Exception</map>
			<defaultType>std::exception</defaultType>
			<contextType>
				<context>Include</context>
				<type>exception</type>
			</contextType>
		</mappingElement>
		<mappingElement>
			<map>String</map>
			<defaultType>std::string</defaultType>			
			<contextType>
				<context>Include</context>
				<type>string</type>
			</contextType>			
		</mappingElement>
		<mappingElement>
			<map>ostream</map>
			<defaultType>std::ostream</defaultType>			
			<contextType>
				<context>Include</context>
				<type>ostream</type>
			</contextType>			
		</mappingElement>
		<mappingElement>
			<map>istream</map>
			<defaultType>std::istream</defaultType>			
			<contextType>
				<context>Include</context>
				<type>istream</type>
			</contextType>			
		</mappingElement>
	</externalTypes>	
</typeMapping>

