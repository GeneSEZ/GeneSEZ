«ENDDEFINE»

«DEFINE GuardBegin FOR MClassifier -»
#ifndef «name.toUpperCase()-»_H_
#define «name.toUpperCase()-»_H_
«ENDDEFINE»

«DEFINE GuardEnd FOR MClassifier -»
#endif /*«name.toUpperCase()-»_H_*/
«ENDDEFINE»

«DEFINE IncludeDeclaration FOR MClassifier -»
