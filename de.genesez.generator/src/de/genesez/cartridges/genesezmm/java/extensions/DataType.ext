// import genesezMM meta model
import genesezMM;


////////////////////////////////////////////////////////////////////////////////
// public extend functions
////////////////////////////////////////////////////////////////////////////////


// cast the uml2 datatypes to java datatypes
getReturnTypeDeclaration(MOperation op) :
	switch (op.returnType.name) {
	case "void" : ""
	case "Void" : ""
	case "int" : op.returnType.name + " returnValue = 0;"
	case "Integer" : op.returnType.name + " returnValue = 0;"
	case "boolean" : op.returnType.name + " returnValue = false;"
	case "Boolean" : op.returnType.name + " returnValue = false;"
	default : op.returnType.name + " returnValue = null;"
	};


getReturnTypeReturn(MOperation op) :
	switch (op.returnType.name) {
	case "void" : ""
	case "Void" : ""
	default : "return returnValue;"
	};
	

